
#include<bits/stdc++.h>         //P3812 luogu
using namespace std;
#define ll long long
#define rep(i,n) for(int i = 0; i < n; ++i)
#define rep2(i,st,ed) for(int i = st; i < ed; ++i)
#define mk(x,y) make_pair(x,y)
#define pb(x) push_back(x)
const ll mod = 1e9 + 7;
const ll N = 2e5 + 200;
const int INF = 0x3f3f3f3f;
const double eps = 1e-7;

ll P[N];
void check(ll x){
    for(int i = 60; i >= 0; i--){
        if(!(x >> (ll)i)){
            continue;
        }
        if(!P[i]){
            P[i] = x;
            break;
        }
        x ^= P[i];
    }
}

int main(){
    ios::sync_with_stdio(false);
    int n;
    cin>>n;
    ll a[N] = {0};
    rep(i,n){
        cin>>a[i];
        check(a[i]);
    }
    ll ans = 0;
    for(int i = 60; i >= 0; i--){
        if((ans ^ P[i]) > ans){
            ans ^= P[i];
        }
    }
    cout<<ans<<endl;
    
    return 0;
}